#include <iostream>

using namespace std;

int main() {
  int n;
  long long factorial = 1; // Usar long long para manejar n√∫meros grandes

  cout << "Ingrese un numero entero: ";
  cin >> n;

  if (n < 0) {
    cout << "El factorial no esta definido para numeros negativos." << endl;
  } else {
    for (int i = 1; i <= n; ++i) {
      factorial *= i;
    }
    cout << "El factorial de " << n << " es: " << factorial << endl;
  }

  return 0;
}
