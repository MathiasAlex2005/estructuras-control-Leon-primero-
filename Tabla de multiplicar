#include <iostream>
using namespace std;

int main() {
  int n;

  // Solicitar número
  cout << "Ingrese un número: ";
  cin >> n;

  // Imprimir tabla de multiplicar
  cout << "Tabla de multiplicar del " << n << endl;
  for (int i = 1; i <= 10; i++) {
    cout << n << " x " << i << " = " << n * i << endl;
  }

  return 0;
}
