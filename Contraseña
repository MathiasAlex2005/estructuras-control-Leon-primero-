#include <iostream>
using namespace std;

int main() {
  string contrasena = "miContrasena"; // Contrase単a correcta
  string intento;

  do {
    cout << "Ingrese la contrase単a: ";
    cin >> intento;

    if (intento != contrasena) {
      cout << "Contrase単a incorrecta. Intente nuevamente." << endl;
    }
  } while (intento != contrasena);

  cout << "Contrase単a correcta. Acceso concedido." << endl;

  return 0;
} 
