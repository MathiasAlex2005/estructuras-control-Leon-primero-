#include <iostream>
using namespace std; 
int main() {
  int num1, num2;

  // Solicitar números
  cout << "Ingrese el primer número: ";
  cin >> num1;
  cout << "Ingrese el segundo número: ";
  cin >> num2;

  // Utilizar sentencia if-else para encontrar el mayor
  if (num1 > num2) {
    cout << "El mayor es: " << num1 <<endl;
  } else if (num2 > num1) {
    cout << "El mayor es: " << num2 <<endl;
  } else {
    cout << "Ambos números son iguales: " << num1 <<endl;
  }

  return 0;
}
